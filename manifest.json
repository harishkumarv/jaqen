{
  "manifest_version": 2,
  "name": "Jaqen",
  "version": "1.0.0",
  "description": "Use this extension to become a faceless man",
  "browser_action": {
    "default_icon": "assets/favicon.png"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "storage",
    "http://*/*",
    "https://*/*"
  ],
  "background":{
    "scripts": ["index.js"],
    "persistent":false
  }
}
